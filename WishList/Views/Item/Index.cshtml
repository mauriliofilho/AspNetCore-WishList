@{
@model List<WishList.Models.Item>
}
<div>
    <h1>Wishlist</h1>
    <ul>
        @foreach (var item in Model)
        {
            <li>@item.Description <a asp-action="delete" asp-route="@item.Id"></a></li>
        }
    </ul>
</div>
